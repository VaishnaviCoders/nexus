{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "regions": ["bom1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "shiksha.cloud"
        }
      ],
      "destination": "https://shiksha.cloud/:path*",
      "permanent": true
    }
  ]
}

